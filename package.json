{
  "name": "mercy-console-log",
  "displayName": "Mercy Console Log",
  "description": "Easy console logging",
  "version": "0.2.2",
  "publisher": "miles-fitzsimons",
  "engines": {
    "vscode": "^1.33.0"
  },
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-go.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.log"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.log",
        "key": "ctrl+shift+q",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "extension.log",
        "title": "Mercy Console Log"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "homepage": "https://github.com/miles-fitzsimons/mercy-console-log/blob/master/README.md"
}
